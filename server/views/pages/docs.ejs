<%- include('../partials/head'); %>

     <body>
          <%- include('../partials/header'); %>
          <% if(flash) { %>
          
               <%- include('./includes/flashMessages'); %>
                    <% }%>
               <div class="container">
                    <!-- <h1 class="my-4 text-center">NodeAuth</h1> -->
                    <div class="row my-5">

                         <div
                              class="col-lg-12 mb-lg-0 text-center d-flex justify-content-center flex-column w-75 mx-auto">

